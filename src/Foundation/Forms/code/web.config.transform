<?xml version="1.0" encoding="utf-8"?>

<configuration  xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.webServer xdt:Transform="InsertIfMissing">
    <handlers  xdt:Transform="InsertIfMissing">
      <add xdt:Transform="InsertIfMissing" xdt:Locator="Match(name)" name="CaptchaImage" verb="*" path="CaptchaImage.axd" type="Sitecore.Form.Core.Pipeline.RequestProcessor.CaptchaResolver, Sitecore.Forms.Core" />
      <add xdt:Transform="InsertIfMissing" xdt:Locator="Match(name)" name="CaptchaAudio" verb="*" path="CaptchaAudio.axd" type="Sitecore.Form.Core.Pipeline.RequestProcessor.CaptchaResolver, Sitecore.Forms.Core" />
    </handlers>
  </system.webServer>
</configuration>